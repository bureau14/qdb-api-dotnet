    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="10px" />
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="10px" />
            <RowDefinition Height="*"/>
            <RowDefinition Height="10px" />
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Label Grid.Column="0" Grid.Row="0" Content="❶"/>
        <Label Grid.Column="1" Grid.Row="0" Content="Cluster URI" />
        <TextBox Grid.Column="2" Grid.Row="0" FontSize="20" Grid.ColumnSpan="2" x:Name="clusterUri">qdb://127.0.0.1:2836</TextBox>
        <Separator Grid.Column="0"  Grid.Row="1" Grid.ColumnSpan="4"></Separator>
        <Label Grid.Column="0" Grid.Row="2" Content="❷"/>
        <Label Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2">Entry alias:</Label>
        <TextBox Grid.Column="2" Grid.Row="2" x:Name="entryAlias">hello.big.data.world</TextBox>
        <Separator Grid.Column="0"  Grid.Row="3" Grid.ColumnSpan="4"></Separator>
        <Label Grid.Column="0" Grid.Row="4" Content="❸"/>
        <StackPanel Grid.Column="1"  Grid.Row="4" Grid.ColumnSpan="3" Orientation="Horizontal" VerticalAlignment="Top">
            <Button Width="150" Click="OnUploadButtonClicked">
                <StackPanel>
                    <Path Style="{StaticResource logo}" Margin="10"/>
                    <Path Style="{StaticResource arrow}"><Path.RenderTransform><ScaleTransform /></Path.RenderTransform></Path>
                    <Label>Upload</Label>
                </StackPanel>
            </Button>
            <Label>or</Label>
            <Button Width="150" Click="OnDownloadButtonClicked">
                <StackPanel>
                    <Path Style="{StaticResource logo}" Margin="10"/>
                    <Path Style="{StaticResource arrow}">
                        <Path.RenderTransform>
                            <ScaleTransform ScaleY="-1" CenterY="15" />
                        </Path.RenderTransform>
                    </Path>
                    <Label>Download</Label>
                </StackPanel>
            </Button>
        </StackPanel>
        <Separator Grid.Column="0"  Grid.Row="5" Grid.ColumnSpan="4"></Separator>
        <ProgressBar Grid.Column="0" Grid.Row="6" Grid.ColumnSpan="3" x:Name="progressBar"/>
        <Button Grid.Column="3" Grid.Row="6" Padding="5,0,5,0" Click="OnCancelClicked">Cancel</Button>
        <Label Grid.Column="0" Grid.Row="6" Grid.ColumnSpan="4" Foreground="Red" x:Name="errorLabel"></Label>
    </Grid>
</Window>
