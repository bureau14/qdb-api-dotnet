<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>dotNET 6</name>
  <description />
  <settings ref="Api_DotNET_Build">
    <disabled-settings>
      <setting-ref ref="RUNNER_6" />
      <setting-ref ref="RUNNER_92" />
    </disabled-settings>
    <parameters>
      <param name="dotnet.framework" value=".NETCoreApp,Version=v6.0" />
      <param name="dotnet.version" value="net6.0" />
      <param name="platform" value="windows-64bit-core2" />
    </parameters>
    <build-runners order="RUNNER_304, RUNNER_306, RUNNER_86, RUNNER_14, RUNNER_310, RUNNER_308, RUNNER_296, RUNNER_21, RUNNER_6, RUNNER_302, RUNNER_92, RUNNER_9, RUNNER_307">
      <runner id="RUNNER_304" name="Set nightly build number based on date" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_noprofile" value="true" />
          <param name="jetbrains_powershell_script_code"><![CDATA[echo "##teamcity[setParameter name='env.BUILD_START_TIMESTAMP' value='$([DateTime]::UtcNow.ToString("yyyyMMddhhmm"))']"
# echo $Env:BUILD_START_TIMESTAMP]]></param>
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_306" name="NuGet Installer" type="jb.nuget.installer">
        <parameters>
          <param name="nuget.path" value="%teamcity.tool.NuGet.CommandLine.DEFAULT%" />
          <param name="nuget.updatePackages" value="" />
          <param name="nuget.updatePackages.mode" value="sln" />
          <param name="nuget.use.restore" value="restore" />
          <param name="sln.path" value="Quasardb.sln" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_14" name="Build" type="dotnet">
        <parameters>
          <param name="command" value="msbuild" />
          <param name="configuration" value="Release" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="msbuild.version" value="MSBuild_CrossPlatform" />
          <param name="paths" value="Quasardb/Quasardb.csproj" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
        </parameters>
      </runner>
      <runner id="RUNNER_310" name="Build Documentation" type="dotnet">
        <parameters>
          <param name="command" value="msbuild" />
          <param name="configuration" value="Release" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="msbuild.version" value="MSBuild_17_Windows" />
          <param name="paths" value="Quasardb.Documentation\Quasardb.Documentation.shfbproj" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_308" name="Restore Tests" type="simpleRunner">
        <parameters>
          <param name="script.content" value="dotnet.exe restore" />
          <param name="teamcity.build.workingDir" value="Quasardb.Tests" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_296" name="Build Tests" type="dotnet">
        <parameters>
          <param name="command" value="msbuild" />
          <param name="configuration" value="%BuildConfiguration%" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="msbuild.version" value="MSBuild_CrossPlatform" />
          <param name="paths" value="Quasardb.Tests/Quasardb.Tests.csproj" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
        </parameters>
      </runner>
      <runner id="RUNNER_6" name="Test - insecure (x86)" type="dotnet">
        <parameters>
          <param name="args" value="/blame" />
          <param name="command" value="vstest" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="framework" value="%dotnet.framework%" />
          <param name="paths" value="&quot;Quasardb.Tests/bin/%BuildConfiguration%/%dotnet.version%/Quasardb.Tests.dll&quot;" />
          <param name="platform" value="x86" />
          <param name="teamcity.step.mode" value="default" />
          <param name="test.settingsFile" value="Quasardb.Tests/insecure.runsettings" />
          <param name="vstest.version" value="VSTest_CrossPlatform" />
        </parameters>
      </runner>
      <runner id="RUNNER_92" name="Test - secure (x86)" type="dotnet">
        <parameters>
          <param name="args" value="/blame" />
          <param name="command" value="vstest" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="framework" value="%dotnet.framework%" />
          <param name="paths" value="&quot;Quasardb.Tests/bin/%BuildConfiguration%/%dotnet.version%/Quasardb.Tests.dll&quot;" />
          <param name="platform" value="x86" />
          <param name="teamcity.step.mode" value="default" />
          <param name="test.settingsFile" value="Quasardb.Tests/secure.runsettings" />
          <param name="vstest.version" value="VSTest_CrossPlatform" />
        </parameters>
      </runner>
      <runner id="RUNNER_307" name="Create NuGet package" type="jb.nuget.pack">
        <parameters>
          <param name="nuget.pack.as.artifact" value="true" />
          <param name="nuget.pack.commandline" value="-NoDefaultExcludes" />
          <param name="nuget.pack.output.clean" value="true" />
          <param name="nuget.pack.output.directory" value="nuget-pack-out" />
          <param name="nuget.pack.project.dir" value="as_is" />
          <param name="nuget.pack.properties" value="&quot;Configuration=Release&quot;" />
          <param name="nuget.pack.specFile" value="Quasardb/Quasardb.csproj" />
          <param name="nuget.path" value="%teamcity.tool.NuGet.CommandLine.DEFAULT%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements>
      <contains id="RQ_1" name="teamcity.agent.jvm.os.name" value="Windows" />
    </requirements>
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="Quasardb_Artifacts_Release_Linux_X8664Core2_Composite" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="*-linux-64bit-core2-c-api.tar.zst!lib/*=&gt;Quasardb/linux" />
      </dependency>
      <dependency id="ARTIFACT_DEPENDENCY_2" sourceBuildTypeId="Quasardb_Artifacts_Release_Windows_X8664Core2" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="*-c-api.zip!**/*=&gt;qdb&#xA;*-server.zip!bin/*=&gt;qdb/bin&#xA;*-utils.zip!bin/*=&gt;qdb/bin&#xA;&#xA;*-windows-64bit-core2-c-api.zip!bin/*=&gt;Quasardb/win64" />
      </dependency>
      <dependency id="ARTIFACT_DEPENDENCY_3" sourceBuildTypeId="Quasardb_Artifacts_Release_Windows_X86Haswell" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="*-windows-32bit-c-api.zip!bin/*=&gt;Quasardb/win32" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Quasardb_Artifacts_Release_Linux_X8664Core2_Composite">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Quasardb_Artifacts_Release_Windows_X8664Core2">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Quasardb_Artifacts_Release_Windows_X86Haswell">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

